{
  "radii": {
    "base": {
      "value": "6",
      "type": "other",
      "description": "the base for our radii tokens"
    },
    "xs": {
      "value": "{radii.base} / 2",
      "type": "borderRadius"
    },
    "sm": {
      "value": "{radii.base}",
      "type": "borderRadius"
    },
    "md": {
      "value": "{radii.sm} + {radii.base}",
      "type": "borderRadius"
    },
    "lg": {
      "value": "{radii.md} + {radii.base}",
      "type": "borderRadius"
    },
    "xl": {
      "value": "{radii.lg} + {radii.base}",
      "type": "borderRadius"
    },
    "2xl": {
      "value": "{radii.xl} + {radii.base}",
      "type": "borderRadius"
    },
    "rounded": {
      "value": "100",
      "type": "borderRadius"
    }
  }
}